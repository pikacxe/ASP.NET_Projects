
@{
    Layout = null;
    int[] data = new int[] { 1, 2, 3, 4, 5, 6};
    List<WebApplication2.Osoba> list = new List<WebApplication2.Osoba>();
    for (int i = 1; i < 11; i++)
    {
            list.Add(new WebApplication2.Osoba(i, "Ime " + i.ToString(), "Prezime "+i.ToString(), "Struka "+i.ToString())); ;
    }
    Random r = new Random();
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <!-- Latest compiled and minified CSS -->
    <style>
        body{
            padding:10px;
            text-align:center;
        }
        img{
            width:400px;
            height:400px;
            border:2px solid black;
        }
        tr,td,table{
            border:2px solid black;
            border-collapse:collapse;
        }
        .tbl{
            width:400px;
            height:400px;
            background-color:#1d67b7;
        }
    </style>
</head>
<body>
    <div>
        @foreach (int i in data)
        {
            Response.Write("<h" + i.ToString() + ">H" + i.ToString() + "</h" + i.ToString() + ">");
        }
        <br />
            @if (r.Next(1, 3) == 1)
            {
                Response.Write("Pogodjena cifra!");
            }
        <br />
        <table class="tbl">
            <thead>
                <tr><td>ID</td><td>Ime</td><td>Prezime</td><td>Struka</td></tr>
            </thead>
            @foreach (var x in list)
            {
                <tr>
                    <td>@x.ID</td>
                    <td>@x.Ime</td>
                    <td>@x.Prezime</td>
                    <td>@x.Struka</td>
                </tr>
            }
        </table>

    </div>
    <img src="./Slike/1.jpg" /> <img src="./Slike/2.jpg" /> <img src="./Slike/3.jpg" />
</body>
</html>
